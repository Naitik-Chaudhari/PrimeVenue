@model IEnumerable<PrimeVenue.Model.Category>

<h2>Select a Category</h2>

<ul>
    @foreach (var category in Model)
    {
        <li>
            <a asp-controller="Category" asp-action="SubCategories" asp-route-categoryId="@category.Id">
                @category.Name
            </a>
        </li>
    }
</ul>
